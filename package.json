{
  "name": "huaweicloudai",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "bunx --bun next dev",
    "build": "bunx --bun next build",
    "start": "bunx --bun next start",
    "lint": "bunx eslint .",
    "compile": "bun build --compile --minify scripts/ts-server-entry.ts --outfile dist/ts-server",
    "build:rag": "cmake -S rag-cpp-server -B rag-cpp-server/build && cmake --build rag-cpp-server/build -j",
    "build:launcher": "cmake -S launcher -B launcher/build && cmake --build launcher/build -j",
    "build:release": "bash scripts/build-release.sh --variant no-license",
    "build:release:no-license": "bash scripts/build-release.sh --variant no-license --out-subdir no-license",
    "build:release:licensed": "bash scripts/build-release.sh --variant licensed --out-subdir licensed",
    "license-server:dev": "cd license-server && bun run dev",
    "test:e2e": "bash scripts/test-e2e.sh",
    "test:rag": "bash scripts/test-rag-relevance.sh",
    "test:launcher": "bash scripts/test-launcher.sh"
  },
  "dependencies": {
    "chromadb": "^3.2.2",
    "glob": "^13.0.0",
    "jszip": "^3.10.1",
    "next": "16.1.4",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "react-markdown": "^10.1.0",
    "remark-gfm": "^4.0.1",
    "ssh2": "^1.17.0",
    "tar": "^7.5.7",
    "undici": "^7.19.0",
    "@xenova/transformers": "^2.17.2"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4",
    "@types/domhandler": "^2.4.5",
    "@types/markdown-it": "^14.1.2",
    "@types/node": "^20",
    "@types/ora": "^3.1.0",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/turndown": "^5.0.6",
    "domhandler": "^5.0.3",
    "eslint": "^9",
    "eslint-config-next": "16.1.4",
    "markdown-it": "^14.1.0",
    "ora": "^9.3.0",
    "tailwindcss": "^4",
    "typescript": "^5"
  }
}
